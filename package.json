  "license": "MIT",
  "dependencies": {
    "bootstrap": "^3.3.2",
    "bootstrap-slider": "^4.4.2",
    "jquery": "^2.1.3",
    "lodash": "^3.1.0",
    "mopidy": "^0.5.0",
    "react": "^0.12.2",
    "react-router": "^0.11.6",
    "reflux": "^0.2.5"
  },
  "devDependencies": {
    "css-loader": "^0.9.1",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.1.0",
    "jsx-loader": "^0.12.2",
    "less": "^2.4.0",
    "less-loader": "^2.0.0",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.5.3",
    "webpack-dev-server": "^1.7.0"
  }
