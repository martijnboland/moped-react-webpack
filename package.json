{
  "name": "moped-react-webpack",
  "version": "0.1.0",
  "description": "A Mopidy Web Client written with ReactJS.",
  "scripts": {
    "build": "node_modules/.bin/webpack --display-error-details",
    "dev": "node_modules/.bin/webpack-dev-server --devtool eval --progress --colors --hot --content-base build",
    "dist": "node_modules/.bin/webpack -p  --config webpack-production.config.js --progress --profile --colors"
  },
